    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "less": "^4.1.1",
    "less-loader": "7.1.0",
    "moment": "^2.29.1",
    "nuxt-socket-io": "^1.1.24",
    "postcss": "^8.4.13",
    "prettier": "^2.0.5",
    "print-js": "^1.6.0",
    "qs": "^6.10.1",
    "stompjs": "^2.3.3",
    "v-viewer": "^1.5.1"
